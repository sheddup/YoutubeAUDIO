{

  "manifest_version": 2,
  "name": "YoutubeAUDIO",
  "version": "1.5.7",
  "homepage_url": "https://github.com/sheddup/YoutubeAUDIO",
  "description": "Listen to Youtube, no video data downloaded.",
  "author": "sheddup",

  "applications": {
    "gecko": {
       "id": "{9be80948-e099-438b-8246-d1b679c89d47}"
     }
  },

  "icons": {
    "48": "icons/extensionLogo.png"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "notifications",
    "*://*.youtube.com/*",
    "*://*.googlevideo.com/*",
    "*://*.google.com/*",
    "*://*.googleapis.com/*",
    "*://*.googlesyndication.com/*",
    "*://*.googletagservices.com/*",
    "*://*.googleadservices.com/*",
    "*://*.google-analytics.com/*",
    "*://*.doubleclick.net/*",
    "*://*.serving-sys.com/*",
    "*://*.atdmt.com/*",
    "*://*.innovid.com/*",
    "*://*.fwmrm.net/*"
  ],

  "background": {
    "scripts": ["scripts/ytaudiomain.js"]
  },

  "content_scripts": [{
    "run_at": "document_start",
    "css": ["scripts/content_scripts/inject.css"],
    "matches": ["*://*.youtube.com/*"]
  }],

  "options_ui": {
    "page": "scripts/options/options.html",
    "browser_style": true
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/ytaudioOFF32.png",
      "32": "icons/ytaudioOFF32.png"
    },
    "default_title": "YoutubeAUDIO Disabled"
  }

}
